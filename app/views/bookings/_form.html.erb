<%= form_for [@listing, @booking] do |f| %>
	<div class="input-daterange input-group" id="datepicker">
		<%= f.text_field :startdate, class: "input-sm form-control" %>
		<span class="input-group-addon">to</span>
		<%= f.text_field :enddate, class: "input-sm form-control"%>
	</div>
	<div>
		<%= f.label :guest %>
		<%= f.number_field :guest %>
	</div>

	<%= f.submit "Submit"%>
<% end %>



